{
  "name": "vue-wind",
  "version": "0.0.7",
  "description": "vue components using tailwind styles",
  "main": "dist/index.js",
  "types": "@types/index.d.ts",
  "scripts": {
    "build": "npm run clean && webpack --mode=production",
    "test": "NODE_ENV=test mochapack --mode=development --webpack-config webpack.config.js --require test/setup.js src/**/*.test.ts",
    "lint": "eslint . --ext .ts,.vue,.js",
    "clean": "rm -rf dist && rm -rf @types",
    "watch": "webpack --mode=production --watch",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htron-dev/vue-wind.git"
  },
  "keywords": [
    "vue",
    "tailwind",
    "components"
  ],
  "author": "Henrique de oliveira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/htron-dev/vue-wind/issues"
  },
  "homepage": "https://github.com/htron-dev/vue-wind#readme",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "@types/chai": "^4.2.11",
    "@types/faker": "^4.1.12",
    "@types/lodash": "^4.14.157",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.12",
    "@types/sinon": "^9.0.4",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "@vuepress/plugin-register-components": "^1.5.2",
    "chai": "^4.2.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "faker": "^4.1.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.0.1",
    "mochapack": "^2.0.2",
    "null-loader": "^4.0.0",
    "postcss-loader": "^3.0.0",
    "sinon": "^9.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "vue": "^2.6.11",
    "vuepress": "^1.5.2",
    "vuepress-plugin-typescript": "^0.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "babel-loader": "^8.1.0",
    "tailwindcss": "^1.4.6",
    "vue-loader": "^15.9.3",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11"
  }
}
