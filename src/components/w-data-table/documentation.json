{
    "components": {
        "w-data-table": {
            "props": {
                "headers": {
                    "description": "The headers of data table",
                    "type": ["any[]"],
                    "default": [],
                    "codeExample": {
                        "name": "header name",
                        "value": "item field name",
                        "class": "extra classes"
                    }
                },
                "items": {
                    "description": "The items to show in table",
                    "type": ["any[]"],
                    "default": []
                },
                "headerItemsDefaultClasses": {
                    "description": "Default classes for th elements in header",
                    "type": ["string", "string[]", "object"],
                    "default": ["px-5", "py-3", "text-left"]
                },
                "itemsTrDefaultClasses": {
                    "description": "Default classes for <tr> elements of items",
                    "type": ["string", "string[]", "object"],
                    "default": ["border-b", "border-t"]
                },
                "itemsTdDefaultClasses": {
                    "description": "Default classes for <td> elements of items",
                    "type": ["string", "string[]", "object"],
                    "default": ["px-5", "py-3"]
                }
            }
        }
    }
}